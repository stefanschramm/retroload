{
  "transform": {"^.+\\.(ts|tsx)$": ["ts-jest"]},
  "moduleNameMapper": {"(.+)\\.js": "$1"},
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/dist/"
  ],
  "transformIgnorePatterns": [],
  "collectCoverageFrom": [
    "src/**/*.{js,jsx,ts,tsx}",
    "!**/*.d.ts",
    "!**/node_modules/**"
  ]
}
